  unset RESPONSE
  echo "I: Script: Open Other EXE:... Selected!"
  RESPONSE=$(zenity --title "Open Other EXE for ${PREFIXNAME}"       \
                    --list --hide-header --width=360 --height=30 \
                    --radiolist --column " "                     \
                    --column "Change"                            \
                        TRUE "Installed Exe (after Setup)"       \
                        0    "Any other Exe"           \
                    --text   "<b>Other exe from...</b>" )
  # If Cancel is clicked
  if [ -z "${RESPONSE}" ]; then
       echo "I: Change Exe: Cancelled!"
       Game_Function
  fi
  
  
  # Print response
  echo "I: Script: Open Other EXE:  ${RESPONSE} Selected!"

  # If only name, append (wine-xx)
  if   [ "${RESPONSE}" = "Installed Exe (after Setup)" ]; then
         # Call CHANGE_INSTALLED_EXE
         source ${WINEZGUI_SCRIPTS}/winezgui-change-installed-exe
         CHANGE_INSTALLED_EXE
         OTHER_EXE="${SELECTED_EXE}"
         echo "I: Script: Open Other EXE:  ${SELECTED_EXE} Selected!"
  elif [ "$RESPONSE" = "Any other Exe" ]; then
         # Open Any Other Exe


       OTHER_EXE="$(zenity --file-filter="*.exe *.msi"  \
                           --file-selection        \
                           --title="Select a File")"

       # If no exe is selected
       if [ -z "${OTHER_EXE}" ]; then
            echo "I: Script: Open Other EXE: Cancelled!"
            #unset SELECTION ; # Empty variable
            Game_Function    ; # Go back to Game Window
       fi
 fi
       # If another exe is selected
       echo "I: Script: Open Other EXE: ${OTHER_EXE} selected"
       base=$(basename "${OTHER_EXE}")
       dire=$(dirname "${OTHER_EXE}")

       # Launch other EXE
       cd "$dire"
       echo "I: Script: Open Other EXE: Launching \"${base}\""
       ${WINE_CMD} "$base"
       echo "I: Script: Open Other EXE... Finished!"
       
       source ${WINEZGUI_SCRIPTS}/winezgui-script-find-installed-exe
       FIND_INSTALLED_EXE

