# winezgui-create-change-prefix-file
CREATE_CHANGE_PREFIX_FILE()
{
  NEW_EXE_FILE="${SELECTED_EXE}"
  NEW_EXE_NAME=$(basename "${SELECTED_EXE}")
  NEW_EXE_NOSP=$(echo ${NEW_EXE_NAME}|tr " " "_");
  NEW_EXE_NOEXE=$(echo ${NEW_EXE_NOSP}|sed "s/\.[Ee][Xx][Ee]//" \
                                      |sed "s/\.[Mm][Ss][Ii]//")

  echo "Prefix:${NEW_EXE_NOEXE}" > "${PREFIXDIR}/Change-Prefix-Name"
  echo "I: Create Prefix File: Created file with Prefix:${NEW_EXE_NOEXE}"
}
