# winezgui-winetricks-gui
WineZGUI_Winetricks_Gui()
{
    echo "--------------------- ${FUNCNAME[0]} -------------------------"
source "${DATADIR}/script-check-variables-loaded-or-not"
  Script_Check_Variables_Loaded_Or_Not
  HEADER="$(basename ${0}): ${FUNCNAME[0]}"
  DBUG "I: $(basename ${0}): Launched ${FUNCNAME[0]}"
  ${WINETRICKS} --gui
  # use this after winetricks fixes bug with arch=
  #${WTRICKS_CMD} arch=win64 prefix=${PREFIXDIR} --gui
  return 0
}
