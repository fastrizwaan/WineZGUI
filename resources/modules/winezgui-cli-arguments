# winezgui-cli-arguments
WineZGUI_Cli_Arguments()
{
  
  source "${DATADIR}/script-check-variables-loaded-or-not"
  Script_Check_Variables_Loaded_Or_Not

  for i in "${ARGV}"; do
     case ${i} in
     -v|--version) echo "${APPVERSION}"; exit; ;;
     -h|--help) source "${DATADIR}/winezgui-show-help; WineZGUI_Show_Help; exit; ;;"
     esac
  done
  DBUG "I: ${HEADER}: CLI Arguments completed"
}
