WineZGUI_Script()
{
  DBUG "I: $(basename ${0}): Launched ${FUNCNAME[0]}"
  export SHOW_SCRIPT_FILE="True"
  source ${DATADIR}/winezgui-list-available-prefix-scripts 
  List_Available_Prefix_Scripts

  # Launch the script
  SCRIPT_NAME="$(echo ${SELECTED_SCRIPT}|grep -E "PROGNAME=")"
  echo "SELECTED_SCRIPT = $SELECTED_SCRIPT"
  export LAUNCH="False"
  
  ${SELECTED_SCRIPT}
}
