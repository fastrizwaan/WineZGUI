# Using this to find scripts where variables are not loaded.
#script-check-variables-loaded-or-not
Script_Check_Variables_Loaded_Or_Not()
{
     # Debug output for Variables created 
     for i in WINE_CMD WTRICKS_CMD WINECFG_CMD WEXPLORER_CMD WBOOT_CMD ICOTOOL_CMD ZENITY_CMD \
          EXIFTOOL_CMD ZSTD_CMD TAR_CMD WGET_CMD WRESTOOL_CMD WHICHWINE; do
          if [ ! -z "${i}" ]; then
               echo -n "."
               #echo "Loaded: $i = ${!i}"
          else
                    echo "Missing: $i"
          fi
     done
     echo ""
}