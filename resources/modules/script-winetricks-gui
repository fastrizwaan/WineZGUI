Script_Winetricks_Gui()
{
  DBUG "I: Script: Launched ${FUNCNAME[0]}"
  echo "---------------------${FUNCNAME[0]}-------------------------"
  source "${WINEZGUI_SCRIPTS}/script-check-variables-loaded-or-not"
  Script_Check_Variables_Loaded_Or_Not
  ${WTRICKS_CMD} --gui
  # use this after winetricks fixes bug with arch=
  #${WTRICKS_CMD} arch=win64 prefix=${PREFIXDIR} --gui
  DBUG "I: Script: ${FUNCNAME[0]}: Winetricks GUI ended..."
}
