#winezgui-remove-broken-links
WineZGUI_Remove_Broken_Links()
{
  DBUG "I: $(basename ${0}): Launched ${FUNCNAME[0]}"
    
  source "${DATADIR}/script-check-variables-loaded-or-not" 2>/dev/null ||\
  source "${WINEZGUI_SCRIPTS}/script-check-variables-loaded-or-not"
  Script_Check_Variables_Loaded_Or_Not

  find ${APPLICATIONSDIR} -maxdepth 1 -iname "${SHORTCUT_PREFIX}*" -xtype l -delete 2>/dev/null 2>/dev/null
  ${UPDATE_DESKTOP_DATABASE} ${APPLICATIONSDIR}
  ${UPDATE_ICON_CACHE}
}
